  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "test runmigration",
      "args": [
        "--migrations-path",
        "./example/migrations",
        "--models-path",
        "./example/models"
      ],
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/runmigration.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "example migration:make",
      "args": [
        "migration:make",
        "--migrations-path",
        "./example/migrations",
        "--models-path",
        "./example/models"
      ],
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/sequelize-mig.mjs"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "test migration:make",
      "args": ["migration:make", "-n", "Hi"],
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/sequelize-mig.mjs"
    }
  ]
}
